{
    "version": 2,
    "builds": [{ "src": "hello.py", "use": "@vercel/python" }],
     "routes": [
      {
        "src": "/(.*)",
        "dest": "hello.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "DATABASE_URL": "postgresql://{db_user}:{db_password}@{db_host}:{db_port}/{db_name}"
    }
  }

  